				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "SELECT \n    a.SK_ID_CURR AS customer_no,\n    MAX(b.AMT_CREDIT) AS high_approval_credit,\n    AVG(b.AMT_CREDIT) AS avg_approval_credit,\n    COUNT(DISTINCT b.NAME_CONTRACT_TYPE) AS count_consumer_loans,\n    COUNT(DISTINCT c.CREDIT_ACTIVE) AS count_credit_active,\n    COUNT(DISTINCT c.CREDIT_DAY_OVERDUE) AS count_credit_overdue,\n    COUNT(DISTINCT CASE WHEN d.DAYS_INSTALMENT > 30 THEN d.DAYS_INSTALMENT END) AS count_payday_30,\n    SUM(a.AMT_INCOME_TOTAL) AS income,\n    COUNT(DISTINCT a.CNT_CHILDREN) AS cnt_children\nFROM \n    dbo.application a\nLEFT JOIN \n    dbo.previous_application b ON a.SK_ID_CURR = b.SK_ID_CURR\nLEFT JOIN \n    dbo.bureau c ON a.SK_ID_CURR = c.SK_ID_CURR\nLEFT JOIN \n    dbo.installment d ON a.SK_ID_CURR = d.SK_ID_CURR\nGROUP BY \n    a.SK_ID_CURR;",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"customer_no"
							]
						},
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
